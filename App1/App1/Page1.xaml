<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.Page1">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="ZnamkaEntry" Placeholder="Známka" />
            <Picker Title="Předmět" WidthRequest="20" x:Name="TestPicker">
            </Picker>
            <Entry x:Name="VahaEntry" Placeholder="Váha" />
            <Button Text="Přidat" Clicked="PridatZnamku"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>